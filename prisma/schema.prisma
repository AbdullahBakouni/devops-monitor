generator client {
  provider = "prisma-client-js"
  output   = "../libs/database/src/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model ServiceEvent {
  id         String   @id @default(cuid())
  service    String
  status     String
  cluster    String?
  message    String?
  createdAt  DateTime @default(now())
}
